{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Raw OHLCV Schema",
  "type": "object",
  "properties": {
    "ticker": {
      "type": "string"
    },
    "date": {
      "type": "string",
      "format": "date-time"
    },
    "open": {
      "type": "number"
    },
    "high": {
      "type": "number"
    },
    "low": {
      "type": "number"
    },
    "close": {
      "type": "number"
    },
    "volume": {
      "type": [
        "number",
        "integer"
      ]
    }
  },
  "required": [
    "ticker",
    "date",
    "open",
    "high",
    "low",
    "close",
    "volume"
  ]
}